    "shortcuts": [
        [
            "qrazybox.bat",
            "qrazybox"
        ]
    ],
    "checkver": {
        "url": "https://api.github.com/repos/Merricx/qrazybox/commits",
        "regex": "([\\d-]+T)(\\d+):(\\d+):(\\d+)",
        "replace": "$1$2.$3.$4"
    },
    "autoupdate": {
        "url": "https://github.com/Merricx/qrazybox/archive/refs/heads/master.zip"
    }
}
